{
  "name": "jojen",
  "version": "0.1.0",
  "description": "Expressive validator for Node and the browser, with a small footprint and awesome performance",
  "main": "jojen.js",
  "scripts": {
    "debug": "node debug node_modules/mocha/bin/_mocha -r test/_setup.js --compilers js:babel-register && eslint lib",
    "test": "node_modules/mocha/bin/_mocha -r test/_setup.js --compilers js:babel-register && eslint lib",
    "bundle": "node_modules/browserify/bin/cmd.js lib/index.js -t babelify --presets es2015 > jojen.js",
    "minify": "node_modules/closurecompiler/bin/ccjs jojen.js --compilation_level=ADVANCED_OPTIMIZATIONS > jojen.min.js",
    "prepublish": "babel lib --out-dir dist && npm run bundle && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WatchBeam/jojen.git"
  },
  "keywords": [
    "joi",
    "hapi",
    "validator",
    "jojen",
    "node",
    "browser"
  ],
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WatchBeam/jojen/issues"
  },
  "homepage": "https://github.com/WatchBeam/jojen#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "closurecompiler": "^1.5.2",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "joi": "^8.0.0",
    "mocha": "^2.3.4",
    "progress": "^1.1.8"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "validator": "^5.0.0"
  }
}
